<h1><%= @merchant.name %>'s Dashboard</h1>
<%= link_to "#{@merchant.name}'s Items", merchant_items_index_path, data: { turbo_method: :get } %>
<%= link_to "#{@merchant.name}'s Invoices", merchant_invoices_index_path, data: { turbo_method: :get } %>

<section id="top-five-customers">
  <% @merchant.top_five_customers.each do |customer|%>
    <div id="customers-<%= customer.id%>">
      <ol>
        <h4>Customer Name: <%= customer.full_name %> Successful Transactions: <%= customer.successful_transactions%></h4>
      </ol>
    </div>
  <% end %>
</section>
